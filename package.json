{
	"name": "mcjspacker-vscode",
	"displayName": "mcjspacker-vscode",
	"description": "VSCode support for mcjspacker",
	"repository": "https://github.com/InfiniteCoder01/mcjspacker-vscode.git",
	"publisher": "InfiniteCoder",
	"readme": "README.md",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.101.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"grammars": [
			{
				"scopeName": "source.mcjs",
				"path": "./syntaxes/injection.json",
				"injectTo": [
					"source.js"
				],
				"embeddedLanguages": {
					"meta.embedded.block.mcjs": "source.mcfunction"
				}
			}
		]
	},
	"activationEvents": [
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0"
	},
	"dependencies": {
		"@spyglassmc/mcfunction": "^0.2.34",
		"regex": "^6.0.1"
	}
}